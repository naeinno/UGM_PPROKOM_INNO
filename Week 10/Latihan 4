daftar=[
    [10,15,20],
    [25,30,35],
    [40,45,50]
]

genap=[num for row in daftar for num in row if num%2==0]
ganjil=[num for row in daftar for num in row if num%2!=0]
total_bilangan_genap=len(genap)
total_bilangan_ganjil=len(ganjil)

print(daftar)
print("Jumlah elemen genap:", total_bilangan_genap)
print("Jumlah elemen ganjil:", total_bilangan_ganjil)
